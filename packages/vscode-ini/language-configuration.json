{
  "comments": {
    "lineComment": ";",
    "blockComment": null
  },
  "brackets": [["[", "]"]],
  "autoClosingPairs": [
    { "open": "[", "close": "]" },
    { "open": "\"", "close": "\"" },
    { "open": "'", "close": "'" }
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*\\[",
      "end": "^(?=\\s*\\[)"
    }
  },
  "indentationRules": {
    "increaseIndentPattern": "^\\s*\\[.*\\]\\s*$",
    "decreaseIndentPattern": "^\\s*\\[.*\\]\\s*$"
  },
  "wordPattern": "[\\w\\-\\.]+"
}
