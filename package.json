{
  "name": "ini-tools",
  "version": "0.0.0",
  "private": true,
  "description": "INI language support tools for VS Code",
  "author": "Leezgion",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Leezgion/ini-tools"
  },
  "scripts": {
    "build": "turbo run build",
    "watch": "turbo run watch --parallel",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rimraf node_modules",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "package": "turbo run package --filter=vscode-ini",
    "publish-ext": "turbo run publish-ext --filter=vscode-ini"
  },
  "devDependencies": {
    "@types/node": "^22.15.0",
    "@types/vscode": "^1.80.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.0",
    "@vscode/vsce": "^3.7.0",
    "esbuild": "^0.27.2",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.0",
    "sharp": "^0.34.5",
    "turbo": "^2.7.0",
    "typescript": "^5.9.0",
    "vitest": "^3.2.0"
  },
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
